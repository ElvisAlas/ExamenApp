<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
            xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
            xmlns:local="clr-namespace:ExamenApp.View"
            xmlns:android="clr-namespace:Microsoft.Maui.Controls.PlatformConfiguration.AndroidSpecific;assembly=Microsoft.Maui.Controls"
            android:TabbedPage.ToolbarPlacement="Bottom"
            x:Class="ExamenApp.AppTabbedPage">

    <NavigationPage Title="Area del círculo" IconImageSource="{StaticResource TabOne}">
        <x:Arguments>
            <local:Circulo Title="Area del círculo" />
        </x:Arguments>
    </NavigationPage>
    <NavigationPage Title="Area del rectángulo" IconImageSource="{StaticResource TabTwo}">
        <x:Arguments>
            <local:Rectangulo Title="Area del rectángulo" />
        </x:Arguments>
    </NavigationPage>

</TabbedPage>
